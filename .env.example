# AWS Troubleshooting Hub Environment Variables
# Copy this file to .env.local and update values as needed

# Application Configuration
REACT_APP_NAME="AWS Troubleshooting Hub"
REACT_APP_VERSION="2.1.0"

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=false

# External Service URLs (optional overrides)
# REACT_APP_AWS_DOCS_BASE_URL=https://docs.aws.amazon.com
# REACT_APP_REPOST_BASE_URL=https://repost.aws

# Development Settings
# REACT_APP_LOG_LEVEL=info